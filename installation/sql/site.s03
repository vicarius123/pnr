\u043e\\u0440 \\u043e\\u0447\\u0435\\u043d\\u044c \\u0434\\u043e\\u0432\\u043e\\u043b\\u0435\\u043d. \\u0425\\u043e\\u0447\\u0435\\u0442 \\u0440\\u0430\\u0437\\u0432\\u0438\\u0432\\u0430\\u0442\\u044c \\u0441\\u0435\\u0442\\u044c \\u0432 \\u041c\\u043e\\u0441\\u043a\\u0432\\u0435 \\u0438 \\u0432 \\u043c\\u0438\\u0440\\u0435.<\\/p>\\r\\n<p class=\\\"MsoNormal\\\"><strong>\\u041c\\u0430\\u043a\\u0441\\u0438\\u043c \\u0412\\u043e\\u0437\\u043d\\u044f\\u043a\\u043e\\u0432<br \\/><\\/strong>\\u0412 \\\"\\u041e\\u0434\\u0435\\u0441\\u0441\\u0435-\\u043c\\u0430\\u043c\\u0435\\\" \\u041c\\u0430\\u043a\\u0441\\u0438\\u043c \\u0412\\u043e\\u0437\\u043d\\u044f\\u043a\\u043e\\u0432 \\u043d\\u0435 \\u0442\\u043e\\u043b\\u044c\\u043a\\u043e \\u0434\\u0438\\u0440\\u0435\\u043a\\u0442\\u043e\\u0440, \\u043d\\u043e \\u0438 \\u0443\\u043f\\u0440\\u0430\\u0432\\u043b\\u044f\\u044e\\u0449\\u0438\\u0439 \\u043f\\u0430\\u0440\\u0442\\u043d\\u0435\\u0440. \\u0422\\u0430\\u043a \\u0445\\u043e\\u0440\\u043e\\u0448\\u043e \\u0443\\u043f\\u0440\\u0430\\u0432\\u0438\\u043b\\u0441\\u044f \\u0441 \\u043d\\u043e\\u0432\\u044b\\u043c \\u043f\\u0440\\u043e\\u0435\\u043a\\u0442\\u043e\\u043c, \\u0447\\u0442\\u043e \\u0432 \\u043a\\u0430\\u0444\\u0435 \\u0441 \\u043f\\u0435\\u0440\\u0432\\u043e\\u0433\\u043e \\u0434\\u043d\\u044f \\u0430\\u0436\\u0438\\u043e\\u0442\\u0430\\u0436.<\\/p>\\r\\n<p class=\\\"MsoNormal\\\"><strong>\\u041e\\u043b\\u044c\\u0433\\u0430 \\u0411\\u0443\\u0431\\u0435\\u043d\\u044c\\u043a\\u043e<br \\/><\\/strong>\\u041e\\u043b\\u044c\\u0433\\u0430 \\u041f\\u0435\\u0442\\u0440\\u043e\\u0432\\u043d\\u0430 \\u0411\\u0443\\u0431\\u0435\\u043d\\u044c\\u043a\\u043e - \\u0448\\u0435\\u0444-\\u043f\\u043e\\u0432\\u0430\\u0440 \\\"\\u041e\\u0434\\u0435\\u0441\\u0441\\u044b-\\u043c\\u0430\\u043c\\u044b\\\". \\u0413\\u043b\\u0430\\u0432\\u043d\\u044b\\u0439 \\u0441\\u043f\\u0435\\u0446 \\u043f\\u043e \\u0442\\u044e\\u043b\\u044c\\u043a\\u0435 \\u0438 \\u0432\\u0430\\u0440\\u0435\\u043d\\u0