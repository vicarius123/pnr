INSERT INTO `#__overrider` VALUES ('1614', 'MOD_ARTICLES_CATEGORY_FIELD_LINKTITLES_DESC', 'Заголовок как ссылка', '/var/www/vhosts/none.ru/odessa-none/language/ru-RU/ru-RU.mod_articles_category.ini'), ('1615', 'MOD_ARTICLES_CATEGORY_FIELD_MODE_DESC', 'Пожалуйста, выберите режим, который вы хотели бы использовать. Если выбрать \'Обычный\', то просто настройте модуль и он будет отображать статичный список статей в пункте меню, к которому будет привязан. Если выбрать \'Динамический\', то вы так же можете настроить модуль, однако значение параметра Категория учитываться не будет. Вместо этого, модуль будет динамически определять представление страницы. Если используется представление \'Список материалов категории\', будет отображать список материалов этой категории. Если выбран \'Динамический\', можно привязать модуль \'ко всем страницам\' сайта. В этом случае он будет автоматически определять, следует ли ему что-либо показывать.', '/var/www/vhosts/none.ru/odessa-none/language/ru-RU/ru-RU.mod_articles_category.ini'), ('1616', 'MOD_ARTICLES_CATEGORY_FIELD_MODE_LABEL', 'Режим', '/var/www/vhosts/none.ru/odessa-none/language/ru-RU/ru-RU.mod_articles_category.ini'), ('1617', 'MOD_ARTICLES_CATEGORY_FIELD_MONTHYEARFORMAT_DESC', 'Введите пожалуйста корректный код формата. Для более подробной информации ознакомьтесь с материалами справочной системы языка PHP: http://php.net/date.', '/var/www/vhosts/none.ru/odessa-none/language/ru-RU/ru-RU.mod_articles_category.ini'), ('1618', 'MOD_ARTICLES_CATEGORY_FIELD_MONTHYEARFORMAT_LABEL', 'Формат показа месяца и года', '/var/www/vhosts/none.ru/odessa-none/language/ru-RU/ru-RU.mod_articles_category.ini'), ('1619', 'MOD_ARTICLES_CATEGORY_FIELD_RELATIVEDATE_DESC', 'Если выше выбран режим \'Относительная дата\', то, введите количество дней. Продолжительность будет автоматически вычислена исходя из текущей даты и указанного вами значения.', '/var/www/vhosts/none.ru/odessa-none/language/ru-RU/ru-RU.mod_articles_category.ini'), ('1620', 'MOD_ARTI